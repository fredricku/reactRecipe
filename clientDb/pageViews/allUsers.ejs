<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>All Users</title>
    <link rel="stylesheet" href="/styles/styles.css" type="text/css">
  </head>
  <body>
    <h1>All users</h1>
    <main>
      <table>
        <thead>
          <tr>
            <th>UserID</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Address</th>
            <th>Email address</th>
            <th>password</th>

          </tr>
        </thead>
        <tbody>
          <% result.forEach(user =>{ %>
            <tr>
              <td><%= user.userId %></td>
              <td><%= user.firstname %></td>
              <td><%= user.lastname %></td>
              <td><%= user.address %></td>
              <td><%= user.emailaddress%></td>
              <td><%= user.password%></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
      <a href="/"><button>Menu</button></a>
    </main>
  </body>
</html>
